    border-top-width: 2px;
    border-bottom-width: 2px;
    border-left-color: rgba(0, 0, 0, 0.17);
    border-right-color: rgba(0, 0, 0, 0.17);
    border-top-color: rgba(0, 0, 0, 0.17);
    border-bottom-color: rgba(0, 0, 0, 0.17);
    margin-top: 2px;
    margin-bottom: 2px;
}

